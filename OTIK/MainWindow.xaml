<Window x:Class="OTIK.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OTIK"
        mc:Ignorable="d"
        Title="MainWindow" Height="826.3" Width="368.2">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="FileFolderText" HorizontalAlignment="Left" Height="65" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="214" IsEnabled="False" Margin="0,10,0,0"/>
        <Button x:Name="folderOpen" Content="Выбрать" HorizontalAlignment="Left" Margin="214,10,0,0" VerticalAlignment="Top" Width="92" Height="65" Click="folderOpen_Click"/>
        <Button x:Name="StartRead" Content="Запускаем ракету" HorizontalAlignment="Left" Margin="0,75,0,0" VerticalAlignment="Top" Width="306" Height="35" Click="StartRead_Click"/>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Height="687" Margin="0,110,-0.4,-0.2" VerticalAlignment="Top" Width="362" Grid.ColumnSpan="2">

        </DataGrid>

    </Grid>
</Window>
